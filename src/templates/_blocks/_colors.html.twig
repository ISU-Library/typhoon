          "300": "yellow-300",
          "400": "yellow-400",
          "500": "yellow-500",
          "600": "yellow-600",
          "700": "yellow-700",
          "800": "yellow-800",
          "900": "yellow-900",
      },
      "Blue": {
          "100": "blue-100",
