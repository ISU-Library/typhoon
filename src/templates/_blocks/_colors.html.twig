%}

<section class="container lib-my96">
  <div class="lib-mb32">
    <h2 class="lib-mb32">Colors</h2>
    <div class="colors--display">
      {% for color, values in colors %}
        <h5>{{ color }}</h5>
        <div class="colors--wrap lib-mb12">
          {% for key, value in colors[color] %}
            <div class="color--info lib-m8">
              <div class="lib-br4 lib-bg-{{ value }}"></div>
              {{ key }}
            </div>
          {% endfor %}
        </div>
      {% endfor %}
    </div>
  </div>
</section>